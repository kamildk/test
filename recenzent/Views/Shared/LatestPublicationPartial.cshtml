@model IEnumerable<recenzent.Data.Model.Publication>
<h3 class="text-center">Najnowsze publikacje</h3>
<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item">@Html.ActionLink(item.Title, "Open", "PublicationPanel", new { id = item.PublicationId }, null)</li>
    }
</ul>